{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "time-tracker",
  "title": "Time Tracker",
  "description": "Simple manual time tracking with local JSON storage",
  "icon": "command-icon.png",
  "author": "your_name",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "log-time",
      "title": "Log Time",
      "description": "Quickly log time spent on a project",
      "mode": "view"
    },
    {
      "name": "view-entries",
      "title": "View Time Entries",
      "description": "View and manage your time entries",
      "mode": "view"
    },
    {
      "name": "manage-projects",
      "title": "Manage Projects",
      "description": "Add, edit, and archive projects",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.65.0",
    "@raycast/utils": "^1.12.0",
    "date-fns": "^3.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
